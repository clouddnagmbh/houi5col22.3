<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		title="Dialog with Search"
		class="sapUiContentPadding" >
		<content>
				<SearchField id="id_search_field" width="100%" search="onSearch"/>
		</content>
		<beginButton>
			<Button text="Close" press="onCloseDialog" />
		</beginButton>
	</Dialog>
</core:FragmentDefinition>